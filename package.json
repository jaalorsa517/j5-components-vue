      "import": "./components-vue/j5-components-vue.js",
      "require": "./components-vue/j5-components-vue.umd.cjs"
    },
    "./style.css": "./components-vue/style.css"
  },
  "author": "jaalorsa517",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaalorsa517/j5-components-vue/issues"
  },
  "homepage": "https://jaalorsa.com/lib-components-vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaalorsa517/j5-components-vue.git"
  },
  "keywords": [
    "vue",
    "component",
    "typescript",
    "javascript"
