{
  "name": "@jaalorsa/j5-components-vue",
  "private": false,
  "version": "2.1.1",
  "type": "module",
  "description": "Libreria de componentes Vue para ayudar al desarrollador en su día a día.",
  "files": [
    "components-vue"
  ],
  "types": "./components-vue/main.d.ts",
